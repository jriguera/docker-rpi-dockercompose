#!/bin/sh
# postrm script
#
# see: dh_installdeb(1)

set -e

# Stop templates
if [ -d /run/systemd/system ] && [ "$1" = remove ]; then
	systemctl stop 'docker-compose-refresh@etc-docker\x2dcompose' >/dev/null || true
	systemctl stop 'docker-compose@etc-docker\x2dcompose' >/dev/null || true
fi

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0
